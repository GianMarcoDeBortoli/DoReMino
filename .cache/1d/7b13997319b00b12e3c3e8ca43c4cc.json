ne":71,"column":25},"source":"modules/table.js","original":{"line":65,"column":10}},{"name":"rowHeight","generated":{"line":71,"column":27},"source":"modules/table.js","original":{"line":65,"column":27}},{"generated":{"line":71,"column":36},"source":"modules/table.js","original":{"line":65,"column":10}},{"generated":{"line":71,"column":37},"source":"modules/table.js","original":{"line":65,"column":2}},{"name":"add_boxes","generated":{"line":72,"column":0},"source":"modules/table.js","original":{"line":66,"column":2}},{"name":"add_boxes","generated":{"line":72,"column":2},"source":"modules/table.js","original":{"line":66,"column":2}},{"generated":{"line":72,"column":11},"source":"modules/table.js","original":{"line":66,"column":11}},{"name":"numRows","generated":{"line":72,"column":12},"source":"modules/table.js","original":{"line":66,"column":12}},{"generated":{"line":72,"column":19},"source":"modules/table.js","original":{"line":66,"column":11}},{"name":"numBoxes","generated":{"line":72,"column":21},"source":"modules/table.js","original":{"line":66,"column":21}},{"generated":{"line":72,"column":29},"source":"modules/table.js","original":{"line":66,"column":11}},{"name":"width","generated":{"line":72,"column":31},"source":"modules/table.js","original":{"line":66,"column":31}},{"generated":{"line":72,"column":36},"source":"modules/table.js","original":{"line":66,"column":11}},{"name":"height","generated":{"line":72,"column":38},"source":"modules/table.js","original":{"line":66,"column":38}},{"generated":{"line":72,"column":44},"source":"modules/table.js","original":{"line":66,"column":11}},{"generated":{"line":72,"column":45},"source":"modules/table.js","original":{"line":66,"column":2}},{"generated":{"line":73,"column":0},"source":"modules/table.js","original":{"line":67,"column":1}}],"sources":{"modules/table.js":"//---------------------------------------- CREATION of TABLE -----------------------------------------------------------\r\n//prende in argomento la const table, il numero di rows e l'altezza di una row.\r\n//crea le rows, le disegna, aggiunge gli attributi per il flex del contenuto e le appendChilda al table.\r\nfunction add_rows(table, num, height) {\r\n  for (let i = 0; i < num; i++) {\r\n      let row = document.createElement(\"div\");\r\n      row.classList.add(\"row\");\r\n      row.id = \"row\"+i;\r\n      row.style.height = height+\"px\";\r\n      if (i%2 == 0) {\r\n          row.style.flexFlow = \"row wrap\";\r\n      } else {\r\n          row.style.flexFlow = \"row-reverse wrap\";\r\n      }\r\n      table.appendChild(row);\r\n  }\r\n}\r\n\r\n//due funzioni ausiliarie chiamate poi dalla add_boxes che disegnano i singoli box e danno gli attributi per il flex di contenuto\r\nfunction horiz_box(box, dim1, dim2) {\r\n  box.classList.add(\"box\");\r\n  box.style.width = dim2+\"px\";\r\n  box.style.height = dim1+\"px\";\r\n}\r\n\r\nfunction vert_box(box, dim1, dim2) {\r\n  box.classList.add(\"box\");\r\n  box.style.width = dim1+\"px\";\r\n  box.style.height = dim2+\"px\";\r\n\r\n}\r\n\r\n//prende in argomento il numero di rows, il numero di boxes per ogni row e le dimensioni di un box\r\n//prende una row alla volta per id, crea l'elemento box, chiama per i primi numBox-1 la horiz_box e per l'ultimo\r\n//di ogni riga la vert_box per disegnarli. Infine assegna il box alla row.\r\nfunction add_boxes(numRows, numBoxes, width, height) {\r\n  let cntbox = 0;\r\n\r\n  for (let i = 0; i < numRows; i++) {\r\n\r\n      let row = document.getElementById(\"row\"+i);\r\n\r\n      for (let j = 0; j < numBoxes-1; j++) {\r\n          let box = document.createElement(\"div\");\r\n          horiz_box(box, width, height);\r\n          box.textContent = cntbox;\r\n          box.id = cntbox;\r\n          cntbox++;\r\n          row.appendChild(box);\r\n      }\r\n\r\n      let box = document.createElement(\"div\");\r\n      vert_box(box, width, height);\r\n      box.textContent = cntbox;\r\n      box.id = cntbox;\r\n      cntbox++;\r\n      row.appendChild(box);\r\n  }\r\n}\r\n\r\n\r\nexport function draw_table(table, tableWidth, tableHeight, numRows, rowHeight, numBoxes, width, height) {\r\n  table.style.width = tableWidth+\"px\";\r\n  table.style.height = tableHeight+\"px\";\r\n  add_rows(table, numRows, rowHeight);\r\n  add_boxes(numRows, numBoxes, width, height)\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"1bdaf60aac260d378497939e4b598762","cacheData":{"env":{}}}